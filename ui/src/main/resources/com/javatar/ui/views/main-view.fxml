<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="800.0" prefWidth="1280.0" stylesheets="@global.css"
            xmlns="http://javafx.com/javafx/11.0.1">
    <children>
        <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
            <children>
                <MenuBar>
                    <menus>
                        <Menu mnemonicParsing="false" text="File">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Mount Cache"/>
                                <MenuItem mnemonicParsing="false" text="Unmount All"/>
                                <MenuItem mnemonicParsing="false" text="Close"/>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Edit">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Delete"/>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Help">
                            <items>
                                <MenuItem mnemonicParsing="false" text="About"/>
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
                <TabPane prefHeight="800.0" prefWidth="1280.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                        <Tab text="File Explorer">
                            <content>
                                <BorderPane>
                                    <center>
                                        <TabPane>
                                            <tabs>
                                                <Tab text="Custom Cache">
                                                    <content>
                                                        <AnchorPane fx:id="fsView"/>
                                                    </content>
                                                </Tab>
                                            </tabs>
                                        </TabPane>
                                    </center>
                                    <left>
                                        <TreeView fx:id="caches" BorderPane.alignment="CENTER"/>
                                    </left>
                                    <right>
                                        <VBox spacing="15.0" BorderPane.alignment="CENTER">
                                            <children>
                                                <Label text="File Information">
                                                    <font>
                                                        <Font size="18.0"/>
                                                    </font>
                                                </Label>
                                                <Label layoutX="10.0" layoutY="10.0" text="CRC:">
                                                    <font>
                                                        <Font size="18.0"/>
                                                    </font>
                                                </Label>
                                                <Label layoutX="10.0" layoutY="31.0" text="Version:">
                                                    <font>
                                                        <Font size="18.0"/>
                                                    </font>
                                                </Label>
                                            </children>
                                            <padding>
                                                <Insets bottom="10.0" left="30.0" right="30.0" top="10.0"/>
                                            </padding>
                                        </VBox>
                                    </right>
                                </BorderPane>
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
            </children>
        </VBox>
    </children>
</AnchorPane>
